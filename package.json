{
  "name": "gogogopher",
  "version": "1.0.0",
  "description": "A secure, RFC-compliant Gopher server written in TypeScript",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": ["gopher", "server", "rfc1436", "typescript"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.18.0",
    "jest": "^30.0.5",
    "prettier": "^3.4.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {}
}